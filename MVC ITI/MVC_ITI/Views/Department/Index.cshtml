@model List<Department>
@{
    ViewData["Title"] = "Index";
    Layout = "_AdvLayout";
}

<h1>Index</h1>
<a href="/department/add" class="btn btn-success">NEW</a>
<div class="col-8">
   <table  class=" table table-hover table-bordered ">
    
         <thead>
             <tr>
         
                 <th>No</th>
                 <th>Name</th>
             </tr>
         </thead>
   
    @foreach(var item in Model)
    {
 
         <tbody>
             <tr>
                 <td>@item.Id</td>
                 <td>@item.Name</td>
             </tr>
         </tbody>

    }
   

    </table>
</div>